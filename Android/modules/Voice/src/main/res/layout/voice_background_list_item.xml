<?xml version="1.0" encoding="utf-8"?>
<io.agora.uiwidget.basic.SquareRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/voice_dialog_bg">
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/action_sheet_background_item_image"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <RelativeLayout
        android:id="@+id/action_sheet_background_item_selected_mask"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/voice_background_mask_border">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_alignParentBottom="true"
            android:background="@drawable/voice_background_check_bg">
            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_centerInParent="true"
                android:src="@drawable/voice_background_ic_check"
                android:scaleType="fitXY"/>
        </RelativeLayout>

    </RelativeLayout>
</io.agora.uiwidget.basic.SquareRelativeLayout>
