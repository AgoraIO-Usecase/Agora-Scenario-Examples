<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:ignore="MissingDefaultResource">


    <com.makeramen.roundedimageview.RoundedImageView
        android:id="@+id/iv_bg"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/club_room_list_item_bg"
        android:scaleType="fitXY"
        android:tint="@android:color/holo_blue_dark"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:riv_corner_radius="10dp" />

    <TextView
        android:id="@+id/tv_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="15dp"
        android:layout_marginTop="15dp"
        android:textColor="@android:color/white"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@id/iv_bg"
        app:layout_constraintStart_toStartOf="@id/iv_bg"
        app:layout_constraintTop_toTopOf="@id/iv_bg"
        tools:text="Show" />

    <LinearLayout
        android:id="@+id/ll_crown"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="@id/tv_name"
        app:layout_constraintTop_toBottomOf="@id/tv_name">

        <ImageView
            android:id="@+id/iv_crown_01"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:src="@drawable/club_room_list_ic_crown" />


        <ImageView
            android:id="@+id/iv_crown_02"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginStart="4dp"
            android:src="@drawable/club_room_list_ic_crown" />

        <ImageView
            android:id="@+id/iv_crown_03"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginStart="4dp"
            android:src="@drawable/club_room_list_ic_crown" />

        <ImageView
            android:id="@+id/iv_crown_04"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginStart="4dp"
            android:src="@drawable/club_room_list_ic_crown" />

        <ImageView
            android:id="@+id/iv_crown_05"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginStart="4dp"
            android:src="@drawable/club_room_list_ic_crown" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_user_01"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@id/iv_bg"
        app:layout_constraintStart_toStartOf="@id/iv_bg"
        app:layout_constraintVertical_chainStyle="packed"
        android:paddingHorizontal="15dp"
        app:layout_constraintBottom_toTopOf="@id/ll_user_02"
        app:layout_constraintTop_toBottomOf="@id/ll_crown"
        android:orientation="horizontal">

        <com.makeramen.roundedimageview.RoundedImageView
            android:id="@+id/iv_user_01"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:src="@drawable/user_profile_image_1"
            android:translationZ="5dp"
            app:riv_border_color="@android:color/white"
            app:riv_border_width="1dp"

            app:riv_corner_radius="99dp" />

        <com.makeramen.roundedimageview.RoundedImageView
            android:id="@+id/iv_user_02"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:src="@drawable/user_profile_image_2"
            android:translationZ="4dp"
            android:layout_marginStart="-6dp"
            app:riv_border_color="@android:color/white"
            app:riv_border_width="1dp"
            app:riv_corner_radius="99dp" />

        <com.makeramen.roundedimageview.RoundedImageView
            android:id="@+id/iv_user_03"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_marginStart="-6dp"
            android:src="@drawable/user_profile_image_2"
            android:translationX="-0dp"
            android:translationZ="3dp"
            app:riv_border_color="@android:color/white"
            app:riv_border_width="1dp"
            app:riv_corner_radius="99dp" />

        <com.makeramen.roundedimageview.RoundedImageView
            android:id="@+id/iv_user_04"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:src="@drawable/user_profile_image_2"
            android:translationZ="2dp"
            app:riv_border_color="@android:color/white"
            android:layout_marginStart="-6dp"
            app:riv_border_width="1dp"
            app:riv_corner_radius="99dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_user_02"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@id/iv_bg"
        app:layout_constraintEnd_toEndOf="@id/iv_bg"
        app:layout_constraintStart_toStartOf="@id/iv_bg"
        android:paddingHorizontal="15dp"
        app:layout_constraintTop_toBottomOf="@id/ll_user_01"
        android:orientation="horizontal">

        <com.makeramen.roundedimageview.RoundedImageView
            android:id="@+id/iv_user_05"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:src="@drawable/user_profile_image_1"
            android:translationZ="5dp"
            app:riv_border_color="@android:color/white"
            app:riv_border_width="1dp"

            app:riv_corner_radius="99dp" />

        <com.makeramen.roundedimageview.RoundedImageView
            android:id="@+id/iv_user_06"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:src="@drawable/user_profile_image_2"
            android:translationZ="4dp"
            android:layout_marginStart="-6dp"
            app:riv_border_color="@android:color/white"
            app:riv_border_width="1dp"
            app:riv_corner_radius="99dp" />

        <com.makeramen.roundedimageview.RoundedImageView
            android:id="@+id/iv_user_07"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_marginStart="-6dp"
            android:src="@drawable/user_profile_image_2"
            android:translationX="-0dp"
            android:translationZ="3dp"
            app:riv_border_color="@android:color/white"
            app:riv_border_width="1dp"
            app:riv_corner_radius="99dp" />

        <com.makeramen.roundedimageview.RoundedImageView
            android:id="@+id/iv_user_08"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:src="@drawable/user_profile_image_2"
            android:translationZ="2dp"
            app:riv_border_color="@android:color/white"
            android:layout_marginStart="-6dp"
            app:riv_border_width="1dp"
            app:riv_corner_radius="99dp" />

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>